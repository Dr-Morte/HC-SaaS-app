<section>
    <div class="intro-title">Contacts</div>
    <table id = contactlist class="tableSection">
      <thead>
        
        <tr>
          <th><span class="text">Name</span>
    
          </th>
          <th><span class="text">Number</span>
    
          </th>
          <th><span class="text">Email</span>
    
          </th>
          </th>
          <th><span class="text">Admin?</span>
          
          </th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user|%>
        <tr>
          <%# This is where each function from the user in the db will go %>
          <td><%= user.username %></td>
          <td>#: <%= user.phone%></td>
          <%# I didn't find anything in the db about storing a user's phone number %>
          <td><%= user.email%></td>
          <td><%= user.admin%></td>
        </tr>
        <% end %>
      </tbody>
      

      
  
  
  
